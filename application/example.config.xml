<?xml version="1.0"?>
<configdata>
  <production>
    <displayErrors>Off</displayErrors>
    <errorsConfig>/path/to/folder/application/config-errors.xml</errorsConfig>
    <formsConfig>/path/to/folder/application/config-forms.xml</formsConfig>
    <cacheDir>/path/to/folder/application/cache</cacheDir>
    <locale>
      <defaultLocale>en_CA</defaultLocale>
      <supportedLocales>en_CA</supportedLocales>
      <tmxLanguagesFile>languages.tmx</tmxLanguagesFile>
    </locale>
    <database>
      <adapter>pdo_mysql</adapter>
      <params>
        <unix_socket>/tmp/mysql.sock</unix_socket>
        <username>username</username>
        <password>password</password>
        <dbname>dbname</dbname>
      </params>
    </database>
    <email>
      <from>
        <name>Nobody</name>
        <address>nobody@example.com</address>
      </from>
      <contact>
        <name>Some Person</name>
        <address>name@example.com</address>
      </contact>
    </email>
    <blogs>
      <blogger>
        <userId>00000000000000000000</userId>
        <conduitBlogId>0000000000000000000</conduitBlogId>
        <divertBlogId>0000000000000000000</divertBlogId>
      </blogger>
      <gamespot>
        <username>username</username>
      </gamespot>
    </blogs>
    <photos>
      <flickrApi>
        <username>username</username>
        <key>00000000000000000000000000000000</key>
        <secret>0000000000000000</secret>
      </flickrApi>
      <galleries>
        <photosPerPage>16</photosPerPage>
      </galleries>
    </photos>
    <music>
      <libraryDir>/path/to/folder</libraryDir>
      <defaultCover>images/defaultCover.jpg</defaultCover>
      <defaultTitle>(none)</defaultTitle>
    </music>
    <search>
      <google>
        <key>00000000000000000000000000000000</key>
        <site>example.com</site>
        <resultsPerPage>10</resultsPerPage>
      </google>
    </search>
  </production>
  <staging extends="production">
    <displayErrors>On</displayErrors>
  </staging>
</configdata>
